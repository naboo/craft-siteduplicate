{
    "name": "naboo/craft-siteduplicate",
    "description": "Site Duplicate let's you duplicate entries across Sites in Craft CMS.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "duplicate",
        "site",
        "sites",
        "entry"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Str√∂mqvist",
            "homepage": "http://www.naboovalley.com"
        }
    ],
    "support": {
        "docs": "https://github.com/naboo/craft-siteduplicate/blob/master/README.md",
        "issues": "https://github.com/naboo/craft-siteduplicate/issues"
    },
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "naboo\\siteduplicate\\": "src/"
        }
    },
    "extra": {
        "name": "Site Duplicate",
        "handle": "siteduplicate",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/naboo/craft-siteduplicate/master/CHANGELOG.md",
        "class": "naboo\\siteduplicate\\SiteDuplicatePlugin"
    }
}
